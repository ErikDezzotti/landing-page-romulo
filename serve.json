{
  "public": ".",
  "directoryListing": false,
  "unlisted": [
    ".DS_Store",
    ".git"
  ],
  "headers": [
    {
      "source": "**/*.html",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "no-store, no-cache, must-revalidate"
        }
      ]
    }
  ],
  "routes": {
    "/": "index.html",
    "/captura": "captura.html",
    "/captura.html": "captura.html",
    "/obrigado": "obrigado.html",
    "/obrigado.html": "obrigado.html"
  }
} 