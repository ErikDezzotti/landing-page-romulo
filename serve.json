{
  "public": ".",
  "directoryListing": false,
  "unlisted": [
    ".DS_Store",
    ".git"
  ],
  "headers": [
    {
      "source": "**/*",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "no-store, no-cache, must-revalidate"
        }
      ]
    }
  ],
  "cleanUrls": true
} 